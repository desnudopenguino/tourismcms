<%= form_for(@venue) do |f| %>
<div class="modal-body">
    <div class="field">
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <div class="field">
      <%= f.label :description %>
      <%= f.text_field :description %>
    </div>
</div>
<div class="modal-footer">
  <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
  <%= content_tag(:button, "Save", :type => :submit, :class => "btn btn-success") %>
</div>
<% end %>
