<%= form_for([@venue,@attraction]) do |f| %>
<div class="modal-body">
  <div class="field">
    <%= f.label :latitude %><br />
    <%= f.text_field :latitude %>
  </div>
  <div class="field">
    <%= f.label :longitude %><br />
    <%= f.text_field :longitude %>
  </div>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </div>
</div>
<div class="modal-footer">
  <button class="btn"data-dismiss="modal" aria-hidden="true">Close</button>
  <%= content_tag(:button, "Save", :type => :submit, :class => "btn btn-success") %>
</div>
<% end %>
