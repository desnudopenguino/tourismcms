<h1>Attractions</h1>
<table class="table table-striped">
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Location</th>
    <th></th>
  </tr>
<% @attractions.each do |attraction| %>
  <tr>
    <td><%= attraction.title %></td>
    <td><%= attraction.description %></td>
    <td><%= "#{attraction.latitude}, #{attraction.longitude}" %></td>
    <td><%= link_to "View", attraction %></td>
  </tr>
<% end %>
</table>
