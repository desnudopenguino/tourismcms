<%= form_for([@venue, @tour]) do |f| %>
<div class="modal-body">
  <div class="field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
</div>
<div class="modal-footer">
  <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
  <%= content_tag(:button, "Save", :type => :sumbit, :class => "btn btn-success") %>
</div>
<% end %>
