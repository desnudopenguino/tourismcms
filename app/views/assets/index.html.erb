<%= link_to @venue.name, @venue %> / Assets 
<h1>Assets</h1>

<table class="table table-striped">
  <tr>
    <th>ID</th>
    <th>Type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @assets.each do |asset| %>
  <tr>
    <td><%= asset.id %></td>
    <td><%= asset.assetType %></td>
    <td><%= link_to 'Preview', asset.location, :target => '_blank' %></td>
    <td><%= link_to 'Edit', edit_venue_asset_path(asset.venue,asset) %></td>
    <td><%= link_to 'Delete', venue_asset_path(asset.venue,asset), method: :delete %></td>
  </tr>
<% end %>
</table>
<%= link_to 'New Asset', new_venue_asset_path %>
